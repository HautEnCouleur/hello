{
  "name": "hello",
  "private": true,
  "dependencies": {
    "bootstrap-sass": "~3.3.5",
    "jasny-bootstrap": "~3.1.3",
    "bootswatch": "v3.3.6+2",
    "modernizr": "~2.8.1",
    "holderjs": "~2.8.2",
    "font-awesome": "~4.4.0",
    "headroom": "https://npmcdn.com/headroom.js@0.8.0/dist/headroom.js",
    "jQuery.headroom": "https://npmcdn.com/headroom.js@0.8.0/dist/jQuery.headroom.js"
  },
  "overrides": {
    "bootstrap-sass": {
      "main": [
        "assets/stylesheets/_bootstrap.scss",
        "assets/fonts/bootstrap/*"
      ]
    },
    "jasny-bootstrap": {
      "main": [
        "scss/_variables.scss",
        "scss/_grid-container-smooth.scss",
        "scss/_button-labels.scss",
        "scss/_nav-tab-alignment.scss",
        "scss/_navmenu.scss",
        "scss/_alerts-fixed.scss",
        "scss/_offcanvas.scss",
        "scss/_rowlink.scss",
        "scss/_fileinput.scss",
        "js/*"
      ]
    },
    "font-awesome": {
      "main": [
        "scss/font-awesome.scss",
        "fonts/*"
      ]
    }
  },
  "devDependencies": {}
}
